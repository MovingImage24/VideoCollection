{
    "name": "movingimage/video-collection",
    "description": "Library to facilitate building video collections from video manager APIs through means of configuration",
    "type": "library",
    "license": "Proprietary",
    "authors": [
        {
            "name": "Ruben Knol",
            "email": "ruben.knol@movingimage.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "http://packagist.mi24.tv/"
        }
    ],
    "config": {
        "secure-http": false
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "movingimage/data-provider": "^2.0.0",
        "movingimage/video-manager-meta": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "MovingImage\\VideoCollection\\": "lib/"
        }
    },
    "require-dev": {
        "movingimage/style-checker": "dev-master"
    }
}
